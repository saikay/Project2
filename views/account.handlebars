<div id="home">
  <nav class="navbar navbar-expand-lg navbar-dark bg-dark navigation">
    <h2 class="navTitle"><a class="navbar-brand" href="/">Fridge Frenzy</a></h2>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav"
      aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNav">
      <ul class="navbar-nav nav-left-overwrite">
        <li class="nav-item">
          <a class="nav-link" href="/favorites">Favorites</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="#">Sign Out</a>
        </li>
      </ul>
    </div>
  </nav>


  <div class="container">
    <div class="row">
      <div class="col-10">
        <br>
        <br>
        <br>
        <br>
        <div class="card mb-4">
          <div class="card-header">
            <h3><strong>Recipe Search</strong></h3>
          </div>
          <div class="card-body">
            <input type="text" id="search-bar" class="form-control">
            <br>
            <div class="text-right">
              <button type="submit" class="btn btn-primary btn-md" id="search-btn"><span class="fa fa-search"></span>
                Input Three Ingredients.</button>
            </div>
          </div>
        </div>
      </div>
    </div>



    <div class="container">
      <div class="row">
        <div class="col-8 offset-2">
          <ul id="example-list" class="list-group">
            {{#each examples}}
            <li data-id="{{this.id}}" class="list-group-item">
              <a href="example/{{this.id}}">{{this.text}}</a>
              <button class="btn btn-danger float-right delete">ï½˜</button>
            </li>
            {{/each}}
          </ul>
        </div>
      </div>
    </div>
  </div>

