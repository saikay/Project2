<div class="container">
  <div class="row">
    <div class="col-8 offset-2">
      <div id="companyName">
        <h1 class="favoritesTitle">Welcome!</h1>
        <form class="clearfix mb-4 createAccount" id="ingredientsForm" action="POST">

          <div class="form-group firstName">
            <label for="first-name" id="firstNameTitle">What Ingredients Do You Have?</label><br><br>
            <input type="text" id="ingredientsList" class="form-control" aria-describedby="example-text" placeholder="insert a comma after each ingredient">
          </div>
          <button id="submit" class="btn btn-primary">Submit</button>
        </form>
      </div>
    </div>
  </div>
</div>

